
@{
    ViewBag.Title = "PrimeNumber";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Số Nguyên Tố Từ 1 đến 100 </h2>
@{ 
    var numb = 1;
    var count = 0;

}
@{ 
    for (numb = 1; numb < 100; ++numb)
    {
        count = 0;
        for (int i = 2; i <= Math.Sqrt(numb); i++)
        {
            if (numb % i == 0)
            {
                count++;
            }
        }
            if (count == 0 && numb > 1)
            {
            <b>@numb</b> 
            }
        }
    }

